{
    "name": "laraboy/lara-qrcode",
    "description": "A flexible and advanced QR Code generator for PHP/ Laravel applications.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Laraboy\\LaraQrcode\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Enabs",
            "email": "davidenabs@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "bacon/bacon-qr-code": "^3.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraBoy\\LaraQrcode\\QrCodeServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
